{
  "multipart": [
    {
      "when": {
        "OR": [
          {
            "east": "none",
            "north": "none",
            "south": "none",
            "west": "none"
          }
        ]
      },
      "apply": {
        "model": "backrooms:block/pyroil_dot"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_side0"
      },
      "when": {
        "north": "side|up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_side_alt0"
      },
      "when": {
        "south": "side|up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_side_alt1",
        "y": 270
      },
      "when": {
        "east": "side|up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_side1",
        "y": 270
      },
      "when": {
        "west": "side|up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_up"
      },
      "when": {
        "north": "up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_up",
        "y": 90
      },
      "when": {
        "east": "up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_up",
        "y": 180
      },
      "when": {
        "south": "up"
      }
    },
    {
      "apply": {
        "model": "backrooms:block/pyroil_up",
        "y": 270
      },
      "when": {
        "west": "up"
      }
    }
  ]
}